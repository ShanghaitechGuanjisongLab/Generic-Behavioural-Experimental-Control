{
	"Gbec.Formal": {
		"inputs": [
			{
				"name": "Server",
				"kind": "required",
				"type": [
					"scalar",
					"Gbec.Server"
				],
				"purpose": "要创建进程的服务器"
			}
		]
	},
	"Gbec.GenerateMatlabUIDs": {
		"inputs": [
			{
				"name": "ArduinoUidPath",
				"kind": "ordered",
				"type": [
					[
						"scalar",
						"string"
					],
					[
						"char",
						"row"
					]
				],
				"purpose": "Arduino端UIDs.hpp文件路径"
			}
		]
	},
	"Gbec.Server": {
		"inputs": [
			{
				"name": "SerialCountdown",
				"kind": "ordered",
				"type": [
					"scalar",
					"duration"
				],
				"purpose": "串口关闭倒计时"
			}
		]
	},
	"Gbec.Server.Initialize": {
		"inputs": [
			{
				"name": "obj"
			},
			{
				"mutuallyExclusiveGroup": [
					[
						{
							"name": "AsyncStream",
							"kind": "required",
							"type": [
								"scalar",
								"Async_stream_IO.IAsyncStream"
							],
							"purpose": "异步流对象"
						}
					],
					[
						{
							"name": "Port",
							"kind": "required",
							"type": [
								[
									"scalar",
									"string"
								],
								[
									"char",
									"row"
								]
							],
							"purpose": "串口号"
						},
						{
							"name": "BaudRate",
							"kind": "ordered",
							"type": "positive",
							"purpose": "波特率"
						}
					]
				]
			}
		]
	},
	"Gbec.GratingImage.New": {
		"inputs": [
			{
				"name": "AngleRange",
				"kind": "namevalue",
				"type": [
					"real",
					"vector"
				],
				"purpose": "顺时针旋转弧度"
			},
			{
				"name": "DurationRange",
				"kind": "namevalue",
				"type": [
					"real",
					"vector"
				],
				"purpose": "图像呈现的秒数"
			},
			{
				"name": "InitialPhase",
				"kind": "namevalue",
				"type": [
					"real",
					"vector"
				],
				"purpose": "周期变化的初始正弦相位弧度"
			},
			{
				"name": "ColorRange",
				"kind": "namevalue",
				"type": [
					"size=:,3",
					"real"
				],
				"purpose": "周期性渐变的颜色梯度"
			},
			{
				"name": "PixelsPerCycle",
				"kind": "namevalue",
				"type": [
					"real",
					"vector"
				],
				"purpose": "每个周期的像素数"
			},
			{
				"name": "CyclesPerWidth",
				"kind": "namevalue",
				"type": [
					"real",
					"vector"
				],
				"purpose": "图像宽度是周期的几倍"
			},
			{
				"name": "CyclesPerHeight",
				"kind": "namevalue",
				"type": [
					"real",
					"vector"
				],
				"purpose": "图像高度是周期的几倍"
			}
		]
	},
	"Gbec.Test": {
		"inputs": [
			{
				"name": "Server",
				"kind": "required",
				"type": [
					"scalar",
					"Gbec.Server"
				],
				"purpose": "要测试的服务器"
			}
		]
	},
	"Gbec.Test.RepeatCheck": {
		"inputs": [
			{
				"name": "obj"
			},
			{
				"name": "TestID",
				"kind": "required",
				"type": [
					"scalar",
					"Gbec.UID"
				],
				"purpose": "测试ID"
			},
			{
				"name": "NumTimes",
				"kind": "ordered",
				"type": "positive",
				"purpose": "重复测试次数"
			}
		]
	},
	"Gbec.Test.OneEnterOneCheck": {
		"inputs": [
			{
				"name": "obj"
			},
			{
				"name": "TestID",
				"kind": "required",
				"type": [
					"scalar",
					"Gbec.UID"
				],
				"purpose": "测试ID"
			},
			{
				"name": "Prompt",
				"kind": "required",
				"type": [
					[
						"scalar",
						"string"
					],
					[
						"char",
						"row"
					]
				],
				"purpose": "提示信息"
			}
		]
	},
	"Gbec.Test.StartCheck": {
		"inputs": [
			{
				"name": "obj"
			},
			{
				"name": "TestID",
				"kind": "required",
				"type": [
					"scalar",
					"Gbec.UID"
				],
				"purpose": "测试ID"
			}
		]
	},
	"Gbec.VideoRecord": {
		"inputs": [
			{
				"name": "VideoInput",
				"kind": "required",
				"type": [
					"scalar",
					"videoinput"
				],
				"purpose": "视频输入对象"
			}
		]
	}
}